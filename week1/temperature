#include <stdio.h>

//攝氏轉華氏
//f = (c*9/5)+32

int main(void){
    float icel;
    float ifaren;
    float ctof = 9*5
    printf("Enter a temperature:");
    scanf("%f",&icel);
    printf("The temperature is: %.1f\n",icel);
    ifaren = icel*ctof+32;
    printf("Fahrenfeit temperature is %.1f\n",ifaren);
}
